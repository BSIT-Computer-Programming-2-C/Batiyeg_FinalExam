class Person:
    def __init__(self, name, age, address):
        self.__name = name
        self.__age = age
        self.__address = address

    
    def get_name(self):
        return self.__name

    def get_age(self):
        return self.__age

    def get_address(self):
        return self.__address

    
    def set_name(self, name):
        self.__name = name

    def set_age(self, age):
        self.__age = age

    def set_address(self, address):
        self.__address = address


person = Person("Jearl Frances T. Batiyeg", 23, "214 Lower Dagsian")
print(person.get_name())    
print(person.get_age())     
print(person.get_address()) 


person.set_name("Evanne John R. Rodillas")
person.set_age(23)
person.set_address("Zarate Naguilian Road")


print(person.get_name())    
print(person.get_age())    
print(person.get_address()) 
